<Page x:Class="nanofromage.Views.NameUserControl"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:nanofromage.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="180" d:DesignWidth="1280"
      Title="ConfirmUserControl"
      Background="White">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="1" FontSize="30" Grid.Column="0" TextBlock.TextAlignment="Center" TextWrapping="WrapWithOverflow" Text="TextBox" />
        <CheckBox Content="CheckBox" FontSize="30" Grid.Row="1" Grid.Column="2" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Black" Click="CheckBox_Click"/>
        <CheckBox Content="CheckBox" FontSize="30" Grid.Row="1" Grid.Column="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Black" Click="CheckBox_Click_1"/>
    </Grid>
</Page>
